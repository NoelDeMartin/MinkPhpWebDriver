{
    "name": "oleg-andreyev/mink-phpwebdriver",
    "description": "MinkPhpWebDriver driver for Mink framework",
    "keywords": [
        "webdriver",
        "w3c",
        "jsonwire",
        "javascript",
        "ajax",
        "testing",
        "browser"
    ],
    "type": "mink-driver",
    "license": "MIT",
    "authors": [
        {
            "name": "Oleg Andreyev",
            "email": "oleg@andreyev.lv",
            "role": "Main Developer"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/oleg-andreyev/driver-testsuite.git",
            "no-api": true
        }
    ],
    "require": {
        "php": ">=7.2",
        "ext-json": "*",
        "behat/mink": "^1.9",
        "php-webdriver/webdriver": "^1.12"
    },
    "require-dev": {
        "roave/security-advisories": "dev-master",
        "mink/driver-testsuite": "dev-upstream-merge",
        "behat/mink-extension": "^2.3"
    },
    "autoload": {
        "psr-4": {
            "OAndreyev\\Mink\\Driver\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "OAndreyev\\Mink\\Tests\\Driver\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "config": {
        "allow-plugins": {
            "bamarni/composer-bin-plugin": true
        }
    }
}
